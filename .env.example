# ====== Crew AI Studio (.env.example) ======
# Rename this file to .env and fill in your values.

# PostgreSQL connection string: postgresql://user:password@host:5432/dbname
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/crew_ai_studio

# LLM Providers
# OpenRouter (supports many models like gpt-4o-mini, claude, mistral, etc.)
OPENROUTER_API_KEY=your_openrouter_key_here
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1

# Google Gemini (Flash 2.0, Pro, etc.)
GEMINI_API_KEY=your_gemini_key_here

# Serper (Web search)
SERPER_API_KEY=your_serper_api_key_here

# Optional: change default model shown in UI
DEFAULT_MODEL=openrouter/gpt-4o-mini
